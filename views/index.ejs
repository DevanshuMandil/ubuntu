      /*
      !!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
      Header Configurations
      !!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
      */
      
      header{
          position: absolute;
          top: 0;
          left: 0;
          z-index: 10;
          width: 100%;
          display: -webkit-box;
          display: -ms-flexbox;
          display: flex;
