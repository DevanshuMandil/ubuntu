          color: white;
      }
      .hero h3{
          font: normal 28px "Open Sans", sans-serif;
          margin-bottom: 40px;
          color: white;
      }
      .hero a.btn{
          padding: 20px 46px;
      }
      .hero-content-area{
          opacity: 0;
          margin-top: 100px;
          -webkit-animation: 1s slidefade 1s forwards;
                  animation: 1s slidefade 1s forwards;
      }
      @-webkit-keyframes slidefade{
          100%{
              opacity: 1;
              margin: 0;
          }
      }
      @keyframes slidefade{
          100%{
              opacity: 1;
              margin: 0;
          }
