          display: -ms-flexbox;
          display: flex;
          margin-right: -15px;
      }
      header nav li{
          margin: 0 15px;
          color: white;
      }
      @media (max-width: 1000px){
        header{
          padding: 20px 50px;
        }
      }
      
      
      @media (max-width: 700px){
        header{
          -webkit-box-orient: vertical;
          -webkit-box-direction: normal;
              -ms-flex-direction: column;
                  flex-direction: column;		
        }
      
