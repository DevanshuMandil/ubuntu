          -webkit-animation: 1s fadein 0.5s forwards;
                  animation: 1s fadein 0.5s forwards;
          opacity: 0.5;
          color: white;
      }
      @-webkit-keyframes fadein{
          100%{
              opacity: 1;
          }
      }
      @keyframes fadein{
          100%{
              opacity: 1;
          }
      }
      
      header h2{
          font-family: "Quicksand, sans-serif";
